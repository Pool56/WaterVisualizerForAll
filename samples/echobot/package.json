{
  "name": "echobot",
  "version": "1.0.0",
  "private": true,
  "description": "Agents echo bot sample",
  "author": "Microsoft",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "npm i",
    "build": "tsc --build",
    "prestart": "npm run build",
    "start": "node --env-file .env ./dist/index.js",
    "prebundle": "npm run build",
    "bundle": "esbuild dist/index.js --bundle --platform=node --metafile=dist/meta.json --outfile=dist/bundle.js --tree-shaking=true --keep-names",
    "docker": "docker build -t botimages.azurecr.io/node-echo-bot:latest .",
    "test-tool": "teamsapptester start",
    "test": "npm-run-all -p -r start test-tool"
  },
  "dependencies": {
    "@microsoft/agents-bot-hosting": "0.1.15",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@microsoft/teams-app-test-tool": "^0.2.6",
    "@types/node": "^22.13.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "keywords": []
}
