{
    "name": "node-application-style",
    "version": "1.0.0",
    "private": true,
    "description": "Bot Builder application style bot sample",
    "author": "Microsoft",
    "license": "MIT",
    "main": "./dist/index.js",
    "scripts": {
    "prebuild": "npm install",
    "build": "tsc --build",
    "prestart": "npm run build",
    "start": "node --env-file .env ./dist/index.js",
    "test-tool": "teamsapptester start",
    "test": "npm-run-all -p -r start test-tool",
    "prebundle": "npm run build",
    "bundle": "esbuild ./src/index.js --bundle --platform=node --metafile=dist/meta.json --outfile=dist/bundle.cjs --tree-shaking=true --keep-names",
    "predocker": "npm run bundle",
    "docker": "docker build -t \"$npm_package_name:$npm_package_version\" ."
  },
  "dependencies": {
    "@microsoft/agents-bot-hosting": "^0.1.40",
    "@microsoft/agents-bot-hosting-storage-blob": "^0.1.40",
    "@microsoft/agents-bot-hosting-storage-cosmos": "^0.1.40",
    "express": "^5.0.1"
  },
  "devDependencies": {
    "@microsoft/teams-app-test-tool": "^0.2.6",
    "@types/node": "^22.13.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2",
    "esbuild": "^0.25.0"
  },
    "keywords": []
  }
